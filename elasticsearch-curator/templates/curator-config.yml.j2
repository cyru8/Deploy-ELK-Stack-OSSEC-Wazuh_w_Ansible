#@SEE https://github.com/sadsfae/ansible-elk/blob/master/install/roles/curator/templates/curator-config.yml.j2
client:
  hosts:
    - 127.0.0.1: {{ elasticsearch_port }}
  url_prefix:
  use_ssl: False
  certificate:
  client_cert:
  client_key:
  ssl_no_validate: True
  http_auth:
  timeout: 30
  master_only: False

logging:
  loglevel: INFO
  logfile: 
  logformat: default
  blacklist: ['elasticsearch', 'urllib3']

